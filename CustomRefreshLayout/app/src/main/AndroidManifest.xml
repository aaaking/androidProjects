<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.crl.zzh.customrefreshlayout">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:name=".ChessApp"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.CoordinatorTest"
            android:theme="@style/AppTheme.WindowTranslucent"/>
        <activity
            android:name=".activity.TestAC"
            android:theme="@style/AppTheme.WindowTranslucent"/>
        <activity android:name=".test.Test"/>
    </application>

</manifest>